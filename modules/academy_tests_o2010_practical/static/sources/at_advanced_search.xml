
{% set testname = prueba -%}
{% set counter = 0 -%}


<!-- QUESTION {{ counter + 1 }}
- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->

{% set counter = counter + 1 -%}
{% set found = search.find_wildcards('[AEIOUÁÉÍÓÚÜ]') -%}
{% set rands = random.integer(0, search.matches + 5, search.matches, 2) -%}

<record id="at_question_o2010p_advs_{{ counter }}" model="at.question">
    <field name="name">¿Cuántas vocales en mayúscula hay en el cuerpo del documento «{{ docname }}»?</field>
    <field name="description"><![CDATA[
Texto de búsqueda:\t[AEIOUÁÉÍÓÚÜ]
Opciones de búsqueda:\tCaracteres comodín
]]></field>
    <field name="at_topic_id" ref="at_topic_office_2010_practical" />
    <field name="at_category_ids" eval="[(4, ref('at_category_o2010p_search'))]" />
    <field name="at_tag_ids" eval="[(4, ref('at_tag_o2010p_advanced_search'))]" />
    <field name="at_level_id" ref="academy_tests.at_level_14" />
</record>

<record id="at_answer_o2010p_advs_{{ counter }}_a" model="at.answer">
    <field name="name">{{ search.matches }}</field>
    <field name="sequence" eval="1" />
    <field name="at_question_id" ref="at_question_o2010p_advs_{{ counter }}" />
    <field name="is_correct" eval="True" />
</record>

<record id="at_answer_o2010p_advs_{{ counter }}_b" model="at.answer">
    <field name="name">{{ rands[0] }}</field>
    <field name="sequence" eval="2" />
    <field name="at_question_id" ref="at_question_o2010p_advs_{{ counter }}" />
    <field name="is_correct" eval="False" />
</record>

<record id="at_answer_o2010p_advs_{{ counter }}_c" model="at.answer">
    <field name="name">{{ rands[1] }}</field>
    <field name="sequence" eval="3" />
    <field name="at_question_id" ref="at_question_o2010p_advs_{{ counter }}" />
    <field name="is_correct" eval="False" />
</record>

<record id="at_answer_o2010p_advs_{{ counter }}_d" model="at.answer">
    <field name="name">Ninguna de las anteriores es correcta</field>
    <field name="sequence" eval="4" />
    <field name="at_question_id" ref="at_question_o2010p_advs_{{ counter }}" />
    <field name="is_correct" eval="False" />
</record>


